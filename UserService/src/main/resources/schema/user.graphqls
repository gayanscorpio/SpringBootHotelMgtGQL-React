type CustomeUser {
    id: ID!
    name: String!
    email: String!
}

input UserInput {
    id: String
    name: String!
    email: String!
}

type Query {
    userById(id: ID!): CustomeUser
    allUsers: [CustomeUser]
    usersByIds(ids: [ID!]!): [CustomeUser!]!
}

type Mutation {
    createUser(customeUser: UserInput!): CustomeUser
}
